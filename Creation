local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Multi-Hub",
   LoadingTitle = "Multi Script Hub",
   LoadingSubtitle = "by Francis",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "MultiHubConfig"
   },
   Discord = {
      Enabled = false,
      Invite = "",
      RememberJoins = true
   },
   KeySystem = false,
   Theme = "AmberGlow"
})

local RayfieldTab = Window:CreateTab("Rayfield ⚡", 4483362458)
RayfieldTab:CreateButton({
   Name = "AntiStun",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/franciscokile3-creator/Antistun/refs/heads/main/Antistun"))()
   end
})

