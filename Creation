   LoadingTitle = "Multi Script Hub",
   LoadingSubtitle = "by Francis",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "MultiHubConfig"
   },
   Discord = {
      Enabled = false,
      Invite = "",
      RememberJoins = true
   },
   KeySystem = false,
   Theme = "AmberGlow"
})

